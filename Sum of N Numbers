#include <stdio.h>


 // } Driver Code Ends
//User function Template for C

int nSum(int n){
    int ans = 0;

    
    ans =n*(n+1)/2;//Write your code here to calculate and return sum of n number.


    return ans;
}

// { Driver Code Starts.
    
int main() {
	
	int t; scanf("%d", &t);
	while(t--) {
        int n;
        scanf("%d", &n);
        int ans = nSum(n);
        printf("%d\n", ans);
    }
	
}  // } Driver Code Ends
