class Solution
{
    // Complete the function
    // a: first number
    // b: second number
    public static int GCD(int a, int b)
    {
        //Your code here
       if(a==b)return a;
       if(a%b==0)return b;
       if(b%a==0)return a;
       if(a>b)
           return GCD(a%b,b);
       else
           return GCD(a,b%a); // find and return GCD of two numbers;
    }
}
